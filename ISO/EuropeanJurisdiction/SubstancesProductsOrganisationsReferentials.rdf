	<owl:Ontology rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/EuropeanJurisdiction/SubstancesProductsOrganisationsReferentials/">
		<rdfs:label>Substances, Products, Organisations and Referentials (SPOR) Ontology</rdfs:label>
		<dct:abstract>The Substances, Products, Organisations and Referentials (SPOR) ontology extends the core IDMP ontologies to define additional content required to support alignment with the European Medicines Agency data management services for substances, products, organisations and referentials.</dct:abstract>
		<dct:license rdf:datatype="&xsd;anyURI">http://opensource.org/licenses/MIT</dct:license>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/EuropeanJurisdiction/EuropeanRegistrationAuthorities/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/EuropeanJurisdiction/EuropeanRegulatoryAgencies/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/AnnotationVocabulary/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Classifiers/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Collections/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Designators/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/ProductsAndServices/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/RegistrationAuthorities/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/TextDatatype/"/>
		<owl:versionIRI rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/20221201/EuropeanJurisdiction/SubstancesProductsOrganisationsReferentials/"/>
		<idmp-chg:hasMaturityLevel rdf:resource="&idmp-chg;Provisional"/>
		<cmns-av:copyright>Copyright (c) 2022 EDM Council, Inc.</cmns-av:copyright>
		<cmns-av:copyright>Copyright (c) 2022 Pistoia Alliance, Inc.</cmns-av:copyright>
	</owl:Ontology>
	
	<owl:NamedIndividual rdf:about="&idmp-eura;EuropeanMedicinesAgencyRegistrationAuthority">
		<cmns-ra:manages rdf:resource="&idmp-spor;ReferentialsManagementServicesRegistry"/>
		<cmns-ra:manages rdf:resource="&idmp-spor;SubstancesManagementServicesRegistry"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-spor;OrganisationManagementServices">
		<rdf:type rdf:resource="&cmns-ra;RegistrationService"/>
		<rdfs:label>Organisation Management Services (OMS)</rdfs:label>
		<skos:definition>services supporting a central dictionary of organisation data in multiple languages</skos:definition>
		<cmns-av:abbreviation>OMS service</cmns-av:abbreviation>
		<cmns-av:directSource rdf:resource="https://spor.ema.europa.eu/"/>
		<cmns-col:isPartOf rdf:resource="&idmp-spor;SubstancesProductsOrganisationsReferentialsDataManagementServices"/>
		<cmns-prd:isProvidedBy rdf:resource="&idmp-eura;EuropeanMedicinesAgencyRegistrationAuthority"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-spor;ProductManagementServices">
		<rdf:type rdf:resource="&cmns-ra;RegistrationService"/>
		<rdfs:label>Product Management Services (PMS)</rdfs:label>
		<skos:definition>services supporting a central dictionary of product data in multiple languages</skos:definition>
		<cmns-av:abbreviation>PMS service</cmns-av:abbreviation>
		<cmns-av:adaptedFrom>https://spor.ema.europa.eu/sporwi/</cmns-av:adaptedFrom>
		<cmns-col:isPartOf rdf:resource="&idmp-spor;SubstancesProductsOrganisationsReferentialsDataManagementServices"/>
		<cmns-prd:isProvidedBy rdf:resource="&idmp-eura;EuropeanMedicinesAgencyRegistrationAuthority"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-spor;ReferentialsManagementServices">
		<rdf:type rdf:resource="&cmns-ra;RegistrationService"/>
		<rdfs:label>Referentials Management Services (RMS)</rdfs:label>
		<skos:definition>services supporting a central dictionary of referentials data in multiple languages</skos:definition>
		<cmns-av:abbreviation>RMS service</cmns-av:abbreviation>
		<cmns-av:directSource rdf:resource="https://spor.ema.europa.eu/"/>
		<cmns-col:isPartOf rdf:resource="&idmp-spor;SubstancesProductsOrganisationsReferentialsDataManagementServices"/>
		<cmns-prd:isProvidedBy rdf:resource="&idmp-eura;EuropeanMedicinesAgencyRegistrationAuthority"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-spor;ReferentialsManagementServicesClassificationScheme">
		<rdf:type rdf:resource="&cmns-cls;ClassificationScheme"/>
		<rdfs:label>Referentials Management Services (RMS) classification scheme</rdfs:label>
		<skos:definition>classification scheme that includes the set of controlled vocabularies used to classify entities included in the Substances Products Organisations Referentials (SPOR) Referentials Management Services (RMS) related services and repositories</skos:definition>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-spor;ReferentialsManagementServicesRegistry">
		<rdf:type rdf:resource="&cmns-ra;Registry"/>
		<rdfs:label>Referentials Management Services (RMS) registry</rdfs:label>
		<skos:definition>central dictionary of referentials data in multiple languages available through the Substances Products Organisations Referentials (SPOR) Referentials Management Services (RMS)</skos:definition>
		<cmns-av:abbreviation>SPOR RMS</cmns-av:abbreviation>
		<cmns-av:abbreviation>SPOR-RMS</cmns-av:abbreviation>
		<cmns-av:directSource rdf:resource="https://spor.ema.europa.eu/"/>
		<cmns-ra:isManagedBy rdf:resource="&idmp-eura;EuropeanMedicinesAgencyRegistrationAuthority"/>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&idmp-spor;SubstanceDomainClassifier">
		<rdfs:subClassOf rdf:resource="&cmns-cls;Classifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-cls;classifies"/>
				<owl:onClass rdf:resource="&idmp-sub;Substance"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>substance domain classifier</rdfs:label>
		<skos:definition>classifier that describes the domain in which a particular substance is used from a Substances Products Organisations Referentials (SPOR) Referentials Management Services (RMS) perspective</skos:definition>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&idmp-spor;SubstanceManagementServices">
		<rdf:type rdf:resource="&cmns-ra;RegistrationService"/>
		<rdfs:label>Substance Management Services (SMS)</rdfs:label>
		<skos:definition>services supporting a central dictionary of substance data in multiple languages</skos:definition>
		<cmns-av:abbreviation>SMS service</cmns-av:abbreviation>
		<cmns-av:directSource rdf:resource="https://spor.ema.europa.eu/"/>
		<cmns-col:isPartOf rdf:resource="&idmp-spor;SubstancesProductsOrganisationsReferentialsDataManagementServices"/>
		<cmns-prd:isProvidedBy rdf:resource="&idmp-eura;EuropeanMedicinesAgencyRegistrationAuthority"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-spor;SubstancesManagementServicesRegistry">
		<rdf:type rdf:resource="&cmns-ra;Registry"/>
		<rdfs:label>Substances Management Services registry</rdfs:label>
		<skos:definition>central dictionary of substance data in multiple languages available through the Substances Products Organisations Referentials (SPOR) Substances Management Services (SMS)</skos:definition>
		<cmns-av:abbreviation>SPOR SMS</cmns-av:abbreviation>
		<cmns-av:abbreviation>SPOR-SMS</cmns-av:abbreviation>
		<cmns-av:directSource rdf:resource="https://spor.ema.europa.eu/"/>
		<cmns-av:explanatoryNote>SMS is currently live as an EMA internal system. Until it is onboarded in the SPOR portal, external users should use the following systems: view and search substance data in EUTCT or IRIS; submit substance change requests in the EMA Service Desk portal; Export of substance data are available at https://spor.ema.europa.eu/web-ui-data-backend/download/csv.</cmns-av:explanatoryNote>
		<cmns-ra:isManagedBy rdf:resource="&idmp-eura;EuropeanMedicinesAgencyRegistrationAuthority"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-spor;SubstancesProductsOrganisationsReferentialsDataManagementServices">
		<rdf:type rdf:resource="&cmns-ra;RegistrationService"/>
		<rdfs:label>Substances Products Organisations Referentials (SPOR) data management services</rdfs:label>
		<skos:definition>data management services for substances, products, organisations and referentials (SPOR) designed to support regulatory activities in the European Union</skos:definition>
		<cmns-av:abbreviation>SPOR data management services</cmns-av:abbreviation>
		<cmns-av:directSource rdf:resource="https://spor.ema.europa.eu/"/>
		<cmns-col:hasPart rdf:resource="&idmp-spor;OrganisationManagementServices"/>
		<cmns-col:hasPart rdf:resource="&idmp-spor;ProductManagementServices"/>
		<cmns-col:hasPart rdf:resource="&idmp-spor;ReferentialsManagementServices"/>
		<cmns-col:hasPart rdf:resource="&idmp-spor;SubstanceManagementServices"/>
		<cmns-prd:isProvidedBy rdf:resource="&idmp-eura;EuropeanMedicinesAgencyRegistrationAuthority"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="https://spor.ema.europa.eu/rmswi/#/lists/100000000004/terms/100000000012">
		<rdf:type rdf:resource="&idmp-spor;SubstanceDomainClassifier"/>
		<rdfs:label xml:lang="de">Anwendung am Menschen</rdfs:label>
		<rdfs:label xml:lang="no">Human bruk</rdfs:label>
		<rdfs:label xml:lang="en">Human use</rdfs:label>
		<rdfs:label xml:lang="sv">Humant bruk</rdfs:label>
		<rdfs:label xml:lang="fr-FR">Usage humain</rdfs:label>
		<rdfs:label xml:lang="it">uso umano</rdfs:label>
		<skos:definition>substance domain classifier for a substance that is authorized for use in human populations</skos:definition>
		<cmns-av:abbreviation xml:lang="de">H</cmns-av:abbreviation>
		<cmns-av:abbreviation xml:lang="en">H</cmns-av:abbreviation>
		<cmns-av:abbreviation xml:lang="fr-FR">H</cmns-av:abbreviation>
		<cmns-av:abbreviation xml:lang="it">H</cmns-av:abbreviation>
		<cmns-av:abbreviation xml:lang="sv">H</cmns-av:abbreviation>
		<cmns-dsg:isDefinedIn rdf:resource="&idmp-spor;ReferentialsManagementServicesClassificationScheme"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="https://spor.ema.europa.eu/rmswi/#/lists/100000000004/terms/100000000013">
		<rdf:type rdf:resource="&idmp-spor;SubstanceDomainClassifier"/>
		<rdfs:label xml:lang="de">Anwendung am Tier</rdfs:label>
		<rdfs:label xml:lang="fr-FR">Usage vétérinaire</rdfs:label>
		<rdfs:label xml:lang="ro">Uz veterinar</rdfs:label>
		<rdfs:label xml:lang="en">Veterinary use</rdfs:label>
		<rdfs:label xml:lang="cs">Veterinární použití</rdfs:label>
		<rdfs:label xml:lang="sv">Veterinärmedicinskt bruk</rdfs:label>
		<rdfs:label xml:lang="no">Veterinær bruk</rdfs:label>
		<rdfs:label xml:lang="lv">Veterinārai lietošanai</rdfs:label>
		<rdfs:label xml:lang="it">uso veterinario</rdfs:label>
		<skos:definition>substance domain classifier for a substance that is authorized for use in animal populations</skos:definition>
		<cmns-av:abbreviation xml:lang="cs">V</cmns-av:abbreviation>
		<cmns-av:abbreviation xml:lang="de">V</cmns-av:abbreviation>
		<cmns-av:abbreviation xml:lang="en">V</cmns-av:abbreviation>
		<cmns-av:abbreviation xml:lang="fr-FR">V</cmns-av:abbreviation>
		<cmns-av:abbreviation xml:lang="it">V</cmns-av:abbreviation>
		<cmns-av:abbreviation xml:lang="sv">V</cmns-av:abbreviation>
		<cmns-dsg:isDefinedIn rdf:resource="&idmp-spor;ReferentialsManagementServicesClassificationScheme"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="https://spor.ema.europa.eu/rmswi/#/lists/100000000004/terms/100000000014">
		<rdf:type rdf:resource="&idmp-spor;SubstanceDomainClassifier"/>
		<rdfs:label xml:lang="de">Anwendung an Mensch und Tier</rdfs:label>
		<rdfs:label xml:lang="en">Human and Veterinary use</rdfs:label>
		<rdfs:label xml:lang="no">Human og veterinær bruk</rdfs:label>
		<rdfs:label xml:lang="sv">Humant och veterinärmedicinskt bruk</rdfs:label>
		<rdfs:label xml:lang="cs">Humánní a veterinární použití</rdfs:label>
		<rdfs:label xml:lang="fr-FR">Usage humain et vétérinaire</rdfs:label>
		<rdfs:label xml:lang="it">uso umano e veterinario</rdfs:label>
		<skos:definition>substance domain classifier for a substance that is authorized for use in human and veterinary populations</skos:definition>
		<cmns-av:abbreviation xml:lang="cs">H&amp;V</cmns-av:abbreviation>
		<cmns-av:abbreviation xml:lang="de">H&amp;V</cmns-av:abbreviation>
		<cmns-av:abbreviation xml:lang="en">H&amp;V</cmns-av:abbreviation>
		<cmns-av:abbreviation xml:lang="fr-FR">H&amp;V</cmns-av:abbreviation>
		<cmns-av:abbreviation xml:lang="it">H&amp;V</cmns-av:abbreviation>
		<cmns-av:abbreviation xml:lang="sv">H&amp;V</cmns-av:abbreviation>
		<cmns-dsg:isDefinedIn rdf:resource="&idmp-spor;ReferentialsManagementServicesClassificationScheme"/>
	</owl:NamedIndividual>

</rdf:RDF>