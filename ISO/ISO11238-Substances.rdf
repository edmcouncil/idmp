		<cmns-col:isMemberOf rdf:resource="&idmp-sub;ISO11238-IsotopicSubstitutionVocabulary"/>
		<cmns-dsg:isDefinedIn rdf:resource="&idmp-sub;ISO11238-IsotopicSubstitutionVocabulary"/>
	</owl:NamedIndividual>
	
