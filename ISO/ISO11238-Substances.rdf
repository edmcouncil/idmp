			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-sub;hasIsotopeSubstitutionType"/>
				<owl:onClass rdf:resource="&idmp-sub;IsotopicSubstitutionClassifier"/>
				<owl:maxQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-id;isIdentifiedBy"/>
				<owl:onClass rdf:resource="&idmp-sub;NuclideIdentifier"/>
				<owl:maxQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-sub;hasNuclideName"/>
				<owl:onClass rdf:resource="&idmp-sub;NuclideName"/>
				<owl:maxQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&idmp-sub;hasHalfLife"/>
