			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-id;isIdentifiedBy"/>
				<owl:onClass rdf:resource="&idmp-mprd;PackageIdentifier"/>
