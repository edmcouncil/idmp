			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;comprises"/>
				<owl:someValuesFrom rdf:resource="&idmp-mprd;PackageDescriptionCodeSegment"/>
