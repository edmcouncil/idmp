				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&idmp-mprd;ReferenceStrength">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&idmp-mprd;hasReferenceSubstance"/>
