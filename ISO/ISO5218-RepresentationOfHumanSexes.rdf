		<owl:imports rdf:resource="https://www.omg.org/spec/MVF/MultipleVocabularyFacility/"/>
		<owl:versionIRI rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/20230901/ISO5218-RepresentationOfHumanSexes/"/>
		<idmp-chg:hasMaturityLevel rdf:resource="&idmp-chg;Provisional"/>
		<cmns-av:copyright>Copyright (c) 2022-2023 EDM Council, Inc.</cmns-av:copyright>
		<cmns-av:copyright>Copyright (c) 2022-2023 Pistoia Alliance, Inc.</cmns-av:copyright>
	</owl:Ontology>
	
	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5218-Female">
		<rdf:type rdf:resource="&idmp-sex;ISO5218-HumanSex"/>
		<rdfs:label>ISO 5218 - Female</rdfs:label>
		<skos:definition>concept descriptor indicating that the human sex is female</skos:definition>
		<skos:note>this concept is a classifier for humans based on their sex being female</skos:note>
		<idmp-dtp:hasDisplayName>Female</idmp-dtp:hasDisplayName>
		<cmns-dsg:hasName rdf:resource="&idmp-sex;ISO5218-Female-Name"/>
		<cmns-txt:hasTextValue>2</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5218-Female-Name">
		<rdf:type rdf:resource="&idmp-sex;ISO5218-HumanSexName"/>
		<rdfs:label>ISO 5218 - Female</rdfs:label>
		<cmns-dsg:isNameOf rdf:resource="&idmp-sex;ISO5218-Female"/>
		<mvf:hasTextualName>Female</mvf:hasTextualName>
		<mvf:isPreferred rdf:datatype="&xsd;boolean">true</mvf:isPreferred>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&idmp-sex;ISO5218-HumanSex">
		<rdfs:subClassOf rdf:resource="&idmp-dtp;ISO21090-ConceptDescriptor"/>
		<rdfs:subClassOf rdf:resource="&cmns-cls;Classifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isIncludedIn"/>
				<owl:hasValue rdf:resource="&idmp-sex;ISO5218-HumanSexVocabulary"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>ISO 5218 - human sex</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:oneOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&idmp-sex;ISO5218-Female">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-sex;ISO5218-Male">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-sex;ISO5218-NotApplicable">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-sex;ISO5218-NotKnown">
					</rdf:Description>
				</owl:oneOf>
			</owl:Class>
		</owl:equivalentClass>
		<skos:definition>concept descriptor and code for the classification of human sex or the availability of information about it</skos:definition>
		<skos:note>Human sex is represented by a one-character numeric code.</skos:note>
		<cmns-av:adaptedFrom>ISO/IEC 5218 - Information technology - Codes for the representation of human sexes, Second edition, 2022-06, clause 4</cmns-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-sex;ISO5218-HumanSexName">
		<rdfs:subClassOf rdf:resource="&cmns-cxtdsg;ContextualName"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isIncludedIn"/>
				<owl:hasValue rdf:resource="&idmp-sex;ISO5218-HumanSexVocabulary"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dsg;isNameOf"/>
				<owl:onClass rdf:resource="&idmp-sex;ISO5218-HumanSex"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>ISO 5218 - human sex name</rdfs:label>
		<skos:definition>concept descriptor name for the classification of human sex or the availability of information about it</skos:definition>
		<cmns-av:adaptedFrom>ISO/IEC 5218 - Information technology - Codes for the representation of human sexes, Second edition, 2022-06, clause 4</cmns-av:adaptedFrom>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5218-HumanSexVocabulary">
		<rdf:type rdf:resource="&idmp-dtp;ControlledVocabulary"/>
		<rdf:type rdf:resource="&cmns-cls;ClassificationScheme"/>
		<rdf:type rdf:resource="&cmns-cds;CodeSet"/>
		<rdfs:label>ISO 5218 human sex vocabulary</rdfs:label>
		<skos:definition>controlled vocabulary defining codes for the representation fo human sex according to ISO 5218 standard</skos:definition>
		<cmns-av:adaptedFrom>ISO/IEC 5218 - Information technology - Codes for the representation of human sexes, Second edition, 2022-06, clause 4</cmns-av:adaptedFrom>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5218-Male">
		<rdf:type rdf:resource="&idmp-sex;ISO5218-HumanSex"/>
		<rdfs:label>ISO 5218 - Male</rdfs:label>
		<skos:definition>concept descriptor indicating that the human sex is male</skos:definition>
		<idmp-dtp:hasDisplayName>Male</idmp-dtp:hasDisplayName>
		<cmns-dsg:hasName rdf:resource="&idmp-sex;ISO5218-Male-Name"/>
		<cmns-txt:hasTextValue>1</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5218-Male-Name">
		<rdf:type rdf:resource="&idmp-sex;ISO5218-HumanSexName"/>
		<rdfs:label>ISO 5218 - Male</rdfs:label>
		<cmns-dsg:isNameOf rdf:resource="&idmp-sex;ISO5218-Male"/>
		<mvf:hasTextualName>Male</mvf:hasTextualName>
		<mvf:isPreferred rdf:datatype="&xsd;boolean">true</mvf:isPreferred>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5218-NotApplicable">
		<rdf:type rdf:resource="&idmp-sex;ISO5218-HumanSex"/>
		<rdfs:label>ISO 5218 - Not applicable</rdfs:label>
		<skos:definition>concept descriptor indicating that the value for human sex is not applicable in the context that the concept is used</skos:definition>
		<idmp-dtp:hasDisplayName>Not applicable</idmp-dtp:hasDisplayName>
		<cmns-dsg:hasName rdf:resource="&idmp-sex;ISO5218-NotApplicable-Name"/>
		<cmns-txt:hasTextValue>9</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5218-NotApplicable-Name">
		<rdf:type rdf:resource="&idmp-sex;ISO5218-HumanSexName"/>
		<rdfs:label>ISO 5218 - Not applicable</rdfs:label>
		<cmns-dsg:isNameOf rdf:resource="&idmp-sex;ISO5218-NotApplicable"/>
		<mvf:hasTextualName>Not applicable</mvf:hasTextualName>
		<mvf:isPreferred rdf:datatype="&xsd;boolean">true</mvf:isPreferred>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5218-NotKnown">
		<rdf:type rdf:resource="&idmp-sex;ISO5218-HumanSex"/>
		<rdfs:label>ISO 5218 - Not known</rdfs:label>
		<skos:definition>concept descriptor indicating that the human sex is not known in the context that the concept is used</skos:definition>
		<idmp-dtp:hasDisplayName>Not known</idmp-dtp:hasDisplayName>
		<cmns-dsg:hasName rdf:resource="&idmp-sex;ISO5218-NotKnown-Name"/>
		<cmns-txt:hasTextValue>0</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5218-NotKnown-Name">
		<rdf:type rdf:resource="&idmp-sex;ISO5218-HumanSexName"/>
		<rdfs:label>ISO 5218 - Not known</rdfs:label>
		<cmns-dsg:isNameOf rdf:resource="&idmp-sex;ISO5218-NotKnown"/>
		<mvf:hasTextualName>Not known</mvf:hasTextualName>
		<mvf:isPreferred rdf:datatype="&xsd;boolean">true</mvf:isPreferred>
	</owl:NamedIndividual>

</rdf:RDF>