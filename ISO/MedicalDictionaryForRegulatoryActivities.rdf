	<owl:Ontology rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/MedicalDictionaryForRegulatoryActivities/">
		<rdfs:label>Medical Dictionary For Regulatory Activities (MedDRA) Ontology</rdfs:label>
		<dct:abstract>Medical Dictionary For Regulatory Activities (MedDRA) ontology extends the core IDMP ontologies to define additional content required to support alignment with the Medical Dictionary For Regulatory Activities (MedDRA) vocabulary developed by the International Council for Harmonisation of Technical Requirements for Pharmaceuticals for Human Use (ICH).</dct:abstract>
		<dct:license rdf:datatype="&xsd;anyURI">https://opensource.org/licenses/MIT</dct:license>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/EuropeanJurisdiction/EuropeanRegistrationAuthorities/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/EuropeanJurisdiction/EuropeanRegulatoryAgencies/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-RegistrationAuthorities/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO21090-HarmonizedDatatypes/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/AnnotationVocabulary/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Classifiers/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/CodesAndCodeSets/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Collections/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/ContextualDesignators/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Designators/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Identifiers/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Organizations/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/RegistrationAuthorities/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/RolesAndCompositions/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/TextDatatype/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/MVF/ISO1087-VocabularyForTermsAndDefinitions/"/>
		<owl:versionIRI rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/20241201/MedicalDictionaryForRegulatoryActivities/"/>
		<idmp-chg:hasMaturityLevel rdf:resource="&idmp-chg;Provisional"/>
		<cmns-av:copyright>Copyright (c) 2022-2024 EDM Council, Inc.</cmns-av:copyright>
		<cmns-av:copyright>Copyright (c) 2022-2024 Pistoia Alliance, Inc.</cmns-av:copyright>
	</owl:Ontology>
	
	<owl:Class rdf:about="&idmp-meddra;MedDRAHighLevelGroupTerm">
		<rdfs:subClassOf rdf:resource="&idmp-meddra;MedDRATerm"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;hasMember"/>
				<owl:allValuesFrom rdf:resource="&idmp-meddra;MedDRAHighLevelTerm"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isMemberOf"/>
				<owl:allValuesFrom rdf:resource="&idmp-meddra;MedDRASystemOrganClass"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-cls;isClassifiedBy"/>
				<owl:hasValue rdf:resource="&idmp-meddra;MedDRATermClassifier-HighLevelGroupTerm"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>MedDRA high level group term</rdfs:label>
		<skos:definition>term that groups related high level terms</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-meddra;MedDRAHighLevelTerm">
		<rdfs:subClassOf rdf:resource="&idmp-meddra;MedDRATerm"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isMemberOf"/>
				<owl:allValuesFrom rdf:resource="&idmp-meddra;MedDRAHighLevelGroupTerm"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;hasMember"/>
				<owl:allValuesFrom rdf:resource="&idmp-meddra;MedDRAPreferredTerm"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-cls;isClassifiedBy"/>
				<owl:hasValue rdf:resource="&idmp-meddra;MedDRATermClassifier-HighLevelTerm"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>MedDRA high level term</rdfs:label>
		<skos:definition>term that groups related preferred terms based upon anatomy, pathology, physiology, etiology or function</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-meddra;MedDRAIdentifier">
		<rdfs:subClassOf rdf:resource="&cmns-ra;RegisteredIdentifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-ra;isRegisteredIn"/>
				<owl:hasValue rdf:resource="&idmp-meddra;MedicalDictionaryForRegulatoryActivities"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-id;identifies"/>
				<owl:onClass rdf:resource="&idmp-meddra;MedDRATerm"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>MedDRA identifier</rdfs:label>
		<skos:definition>identifier for terms defining medical condition that are registered in the Medical Dictionary for Regulatory Activities registry</skos:definition>
		<skos:note>The MedDRA code is an eight digit numeric code. MedDRA codes are unique and are never reused.</skos:note>
		<cmns-av:synonym>MedDRA code</cmns-av:synonym>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-meddra;MedDRALowestLevelTerm">
		<rdfs:subClassOf rdf:resource="&idmp-dtp;ISO21090-ConceptDescriptor"/>
		<rdfs:subClassOf rdf:resource="&idmp-meddra;MedDRATerm"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-cls;isClassifiedBy"/>
				<owl:hasValue rdf:resource="&idmp-meddra;MedDRATermClassifier-LowestLevelTerm"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isMemberOf"/>
				<owl:onClass rdf:resource="&idmp-meddra;MedDRAPreferredTerm"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>MedDRA lowest level term</rdfs:label>
		<skos:definition>most specific designation (term) that is registered in the Medical Dictionary for Regulatory Activities registry</skos:definition>
		<skos:note>MedDRA LLTs can be used in ISO IDMP as concept descriptors in many places. Some jurisdictions require the use of MedDRA LLTs for specific properties in ISO IDMP.</skos:note>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&idmp-meddra;MedDRAMaintenanceAndSupportServicesOrganization">
		<rdf:type rdf:resource="&cmns-org;Organization"/>
		<rdfs:label>MedDRA Maintenance and Support Services Organization</rdfs:label>
		<skos:definition>organization under the oversight of the ICH MedDRA Management Committee, the key function of the MSSO is to maintain, distribute, and support MedDRA on behalf of MedDRA users</skos:definition>
		<cmns-av:abbreviation>MSSO</cmns-av:abbreviation>
		<cmns-av:abbreviation>MedDRA MSSO</cmns-av:abbreviation>
		<cmns-av:adaptedFrom>https://www.meddra.org/about-meddra/organisation/msso</cmns-av:adaptedFrom>
		<cmns-org:isSubUnitOf rdf:resource="&idmp-ra;InternationalCouncilForHarmonisationOfTechnicalRequirementsForPharmaceuticalsForHumanUse"/>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&idmp-meddra;MedDRAPreferredTerm">
		<rdfs:subClassOf rdf:resource="&idmp-dtp;ISO21090-ConceptDescriptor"/>
		<rdfs:subClassOf rdf:resource="&idmp-meddra;MedDRATerm"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isMemberOf"/>
				<owl:allValuesFrom>
					<owl:Class>
						<owl:unionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&idmp-meddra;MedDRAHighLevelTerm">
							</rdf:Description>
							<rdf:Description rdf:about="&idmp-meddra;MedDRAPreferredTerm">
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:allValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;hasMember"/>
				<owl:hasSelf rdf:datatype="&xsd;boolean">true</owl:hasSelf>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-cls;isClassifiedBy"/>
				<owl:hasValue rdf:resource="&idmp-meddra;MedDRATermClassifier-PreferredTerm"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;hasMember"/>
				<owl:someValuesFrom rdf:resource="&idmp-meddra;MedDRALowestLevelTerm"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>MedDRA preferred term</rdfs:label>
		<skos:definition>distinct descriptor (single medical concept) for a symptom, sign, disease diagnosis, therapeutic indication, investigation, surgical or medical procedure, and medical social or family history characteristic</skos:definition>
		<skos:note>Each preferred term contains at least itself as lower level term.</skos:note>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&idmp-meddra;MedDRARegistrationAuthority">
		<rdf:type rdf:resource="&cmns-ra;RegistrationAuthority"/>
		<rdfs:label>MedDRA registration authority</rdfs:label>
		<skos:definition>Registration authority (RA) for registration of terms or concepts in the Medical Dictionary for Regulatory Activities (MedDRA) played by the MedDRA Maintenance and Support Services Organization (MSSO)</skos:definition>
		<cmns-av:abbreviation>MedDRA RA</cmns-av:abbreviation>
		<cmns-org:manages rdf:resource="&idmp-meddra;MedicalDictionaryForRegulatoryActivities"/>
		<cmns-rlcmp:isPlayedBy rdf:resource="&idmp-meddra;MedDRAMaintenanceAndSupportServicesOrganization"/>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&idmp-meddra;MedDRASystemOrganClass">
		<rdfs:subClassOf rdf:resource="&idmp-meddra;MedDRATerm"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;hasMember"/>
				<owl:allValuesFrom rdf:resource="&idmp-meddra;MedDRAHighLevelGroupTerm"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-cls;isClassifiedBy"/>
				<owl:hasValue rdf:resource="&idmp-meddra;MedDRATermClassifier-SystemOrganClass"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>MedDRA system organ class</rdfs:label>
		<skos:definition>grouping of higher level group terms by etiology, manifestation site or purpose</skos:definition>
		<skos:note>In addition, there is a System Organ Class (SOC) to contain issues pertaining to products and one to contain social circumstances.</skos:note>
		<cmns-av:abbreviation>SOC</cmns-av:abbreviation>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-meddra;MedDRATerm">
		<rdfs:subClassOf rdf:resource="&cmns-cxtdsg;ContextualDesignation"/>
		<rdfs:subClassOf rdf:resource="&mvf-trm;Term"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-ra;isRegisteredIn"/>
				<owl:hasValue rdf:resource="&idmp-meddra;MedicalDictionaryForRegulatoryActivities"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-cls;isClassifiedBy"/>
				<owl:someValuesFrom rdf:resource="&idmp-meddra;MedDRATermClassifier"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>MedDRA term</rdfs:label>
		<skos:definition>designation, term or concept that is registered in the Medical Dictionary for Regulatory Activities registry</skos:definition>
		<skos:note>The MedDRA term name is a 100 character alphanumeric field used to describe the concept or term.</skos:note>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&idmp-meddra;MedDRATermClassificationScheme">
		<rdf:type rdf:resource="&cmns-cls;ClassificationScheme"/>
		<rdf:type rdf:resource="&cmns-cds;CodeSet"/>
		<rdfs:label>Medical Dictionary for Regulatory Activities term classification scheme</rdfs:label>
		<skos:definition>classification scheme used in MedDRA for classifying medical terms by hierarchical order</skos:definition>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&idmp-meddra;MedDRATermClassifier">
		<rdfs:subClassOf rdf:resource="&cmns-cls;Classifier"/>
		<rdfs:subClassOf rdf:resource="&cmns-cds;CodeElement"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isMemberOf"/>
				<owl:hasValue rdf:resource="&idmp-meddra;MedDRATermClassificationScheme"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dsg;isDefinedIn"/>
				<owl:hasValue rdf:resource="&idmp-meddra;MedDRATermClassificationScheme"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-cls;classifies"/>
				<owl:someValuesFrom rdf:resource="&idmp-meddra;MedDRATerm"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>MedDRA term classifier</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:oneOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&idmp-meddra;MedDRATermClassifier-HighLevelGroupTerm">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-meddra;MedDRATermClassifier-HighLevelTerm">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-meddra;MedDRATermClassifier-LowestLevelTerm">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-meddra;MedDRATermClassifier-PreferredTerm">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-meddra;MedDRATermClassifier-SystemOrganClass">
					</rdf:Description>
				</owl:oneOf>
			</owl:Class>
		</owl:equivalentClass>
		<skos:definition>classifier indicating that a term appears at a particular level in the hierarchy of terms in the Medical Dictionary for Regulatory Activities registry</skos:definition>
		<skos:note>Multiple MedDRA terms may be classified at the same level.</skos:note>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&idmp-meddra;MedDRATermClassifier-HighLevelGroupTerm">
		<rdf:type rdf:resource="&idmp-meddra;MedDRATermClassifier"/>
		<rdf:type rdf:resource="&cmns-cds;CodeElement"/>
		<rdfs:label>MedDRA term classifier - high level group term</rdfs:label>
		<skos:definition>classifier for terms that groups together related High Level Terms (HLGT)</skos:definition>
		<cmns-av:acronym>HLGT</cmns-av:acronym>
		<cmns-av:adoptedFrom>https://www.meddra.org/how-to-use/basics/hierarchy</cmns-av:adoptedFrom>
		<cmns-txt:hasTextValue>HLGT</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-meddra;MedDRATermClassifier-HighLevelTerm">
		<rdf:type rdf:resource="&idmp-meddra;MedDRATermClassifier"/>
		<rdf:type rdf:resource="&cmns-cds;CodeElement"/>
		<rdfs:label>MedDRA term classifier - high level term</rdfs:label>
		<skos:definition>classifier for terms that group together related Preferred Terms (PT)s based upon anatomy, pathology, physiology, etiology or function. HLTs, related to each other by anatomy, pathology, physiology, etiology or function</skos:definition>
		<cmns-av:acronym>HLT</cmns-av:acronym>
		<cmns-av:adoptedFrom>https://www.meddra.org/how-to-use/basics/hierarchy</cmns-av:adoptedFrom>
		<cmns-txt:hasTextValue>HLT</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-meddra;MedDRATermClassifier-LowestLevelTerm">
		<rdf:type rdf:resource="&idmp-meddra;MedDRATermClassifier"/>
		<rdf:type rdf:resource="&cmns-cds;CodeElement"/>
		<rdfs:label>MedDRA term classifier - lowest level term</rdfs:label>
		<skos:definition>classifier for terms that are the most specific level in the MedDRA hierarchy</skos:definition>
		<cmns-av:acronym>LLT</cmns-av:acronym>
		<cmns-av:adoptedFrom>https://www.meddra.org/how-to-use/basics/hierarchy</cmns-av:adoptedFrom>
		<cmns-txt:hasTextValue>LLT</cmns-txt:hasTextValue>
