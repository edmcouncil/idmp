<rdf:RDF xml:base="https://spec.pistoiaalliance.org/idmp/ontology/ISO/NorthAmericanJurisdiction/CommonTerminologyCriteriaForAdverseEvents/"
	xmlns:cmns-av="https://www.omg.org/spec/Commons/AnnotationVocabulary/"
	xmlns:cmns-cds="https://www.omg.org/spec/Commons/CodesAndCodeSets/"
	xmlns:cmns-cls="https://www.omg.org/spec/Commons/Classifiers/"
	xmlns:cmns-col="https://www.omg.org/spec/Commons/Collections/"
	xmlns:cmns-dsg="https://www.omg.org/spec/Commons/Designators/"
	xmlns:cmns-ra="https://www.omg.org/spec/Commons/RegistrationAuthorities/"
	xmlns:cmns-txt="https://www.omg.org/spec/Commons/TextDatatype/"
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:idmp-chg="https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/"
	xmlns:idmp-ctcae="https://spec.pistoiaalliance.org/idmp/ontology/ISO/NorthAmericanJurisdiction/CommonTerminologyCriteriaForAdverseEvents/"
	xmlns:idmp-dtp="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO21090-HarmonizedDatatypes/"
	xmlns:idmp-eura="https://spec.pistoiaalliance.org/idmp/ontology/ISO/EuropeanJurisdiction/EuropeanRegistrationAuthorities/"
	xmlns:idmp-nara="https://spec.pistoiaalliance.org/idmp/ontology/ISO/NorthAmericanJurisdiction/NorthAmericanRegistrationAuthorities/"
	xmlns:mvf="https://www.omg.org/spec/MVF/MultipleVocabularyFacility/"
	xmlns:mvf-trm="https://www.omg.org/spec/MVF/ISO1087-VocabularyForTermsAndDefinitions/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/NorthAmericanJurisdiction/CommonTerminologyCriteriaForAdverseEvents/">
		<rdfs:label>Common Terminology Criteria for Adverse Events (CTCAE) v5.0 Ontology</rdfs:label>
		<dct:abstract>The Common Terminology Criteria for Adverse Events (CTACE) Ontology ontology extends the core IDMP ontologies to support the describe grades of adverse events using the CTCAE vocabulary.</dct:abstract>
		<dct:license rdf:datatype="&xsd;anyURI">https://opensource.org/licenses/MIT</dct:license>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO21090-HarmonizedDatatypes/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/NorthAmericanJurisdiction/NorthAmericanRegistrationAuthorities/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/AnnotationVocabulary/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Classifiers/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/CodesAndCodeSets/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Collections/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Designators/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/RegistrationAuthorities/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/TextDatatype/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/MVF/ISO1087-VocabularyForTermsAndDefinitions/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/MVF/MultipleVocabularyFacility/"/>
		<owl:versionIRI rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/20230815/NorthAmericanJurisdiction/CommonTerminologyCriteriaForAdverseEvents/"/>
		<idmp-chg:hasMaturityLevel rdf:resource="&idmp-chg;Provisional"/>
		<cmns-av:copyright>Copyright (c) 2022-2023 EDM Council, Inc.</cmns-av:copyright>
		<cmns-av:copyright>Copyright (c) 2022-2023 Pistoia Alliance, Inc.</cmns-av:copyright>
	</owl:Ontology>
	
	<owl:Class rdf:about="&idmp-ctcae;CTCAE-AdverseEventName">
		<rdfs:subClassOf rdf:resource="&cmns-dsg;Name"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dsg;isNameOf"/>
				<owl:allValuesFrom rdf:resource="&idmp-ctcae;CTCAE-AdverseEventTerm"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isIncludedIn"/>
				<owl:hasValue rdf:resource="&idmp-ctcae;CommonTerminologyCriteriaForAdverseEventsVocabulary"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Common Terminology Criteria for Adverse Events adverse event name</rdfs:label>
		<skos:definition>name denoting an adverse event term in the CTCAE vocabulary</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-ctcae;CTCAE-AdverseEventTerm">
		<rdfs:subClassOf rdf:resource="&idmp-eura;MedDRATerm"/>
		<rdfs:subClassOf rdf:resource="&idmp-dtp;ISO21090-ConceptDescriptor"/>
		<rdfs:subClassOf rdf:resource="&cmns-cls;Classifier"/>
		<rdfs:subClassOf rdf:resource="&mvf-trm;Term"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-ra;isRegisteredIn"/>
				<owl:hasValue rdf:resource="&idmp-eura;MedicalDictionaryForRegulatoryActivities"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isIncludedIn"/>
				<owl:hasValue rdf:resource="&idmp-ctcae;CommonTerminologyCriteriaForAdverseEventsVocabulary"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Common Terminology Criteria for Adverse Events adverse event term</rdfs:label>
		<skos:definition>term defined in the Common Terminology Criteria for Adverse Events terminology that is used to describe adverse events (AE)</skos:definition>
		<cmns-av:abbreviation>CTCAE adverse event term</cmns-av:abbreviation>
		<cmns-av:adoptedFrom>https://ctep.cancer.gov/protocoldevelopment/electronic_applications/docs/CTCAE_v5_Quick_Reference_5x7.pdf</cmns-av:adoptedFrom>
		<cmns-av:explanatoryNote>An Adverse Event (AE) is any unfavorable and unintended sign (including an abnormal laboratory finding), symptom, or disease temporally associated with the use of a medical treatment or procedure that may or may not be considered related to the medical treatment or procedure.</cmns-av:explanatoryNote>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&idmp-ctcae;CTCAE-Grade1">
		<rdf:type rdf:resource="&idmp-ctcae;CTCAE-GradeTerm"/>
		<rdfs:label>Grade 1</rdfs:label>
		<skos:definition>mild; asymptomatic or mild symptoms; clinical or diagnostic observations only; intervention not indicated</skos:definition>
		<idmp-dtp:hasDisplayName>Grade 1</idmp-dtp:hasDisplayName>
		<cmns-av:directSource>https://ctep.cancer.gov/protocoldevelopment/electronic_applications/docs/CTCAE_v5_Quick_Reference_5x7.pdf</cmns-av:directSource>
		<cmns-txt:hasTextValue>Grade 1</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-ctcae;CTCAE-Grade2">
		<rdf:type rdf:resource="&idmp-ctcae;CTCAE-GradeTerm"/>
		<rdfs:label>Grade 2</rdfs:label>
		<skos:definition>moderate; minimal, local or noninvasive intervention indicated; limiting age-appropriate instrumental ADL</skos:definition>
		<idmp-dtp:hasDisplayName>Grade 2</idmp-dtp:hasDisplayName>
		<cmns-av:directSource>https://ctep.cancer.gov/protocoldevelopment/electronic_applications/docs/CTCAE_v5_Quick_Reference_5x7.pdf</cmns-av:directSource>
		<cmns-txt:hasTextValue>Grade 2</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-ctcae;CTCAE-Grade3">
		<rdf:type rdf:resource="&idmp-ctcae;CTCAE-GradeTerm"/>
		<rdfs:label>Grade 3</rdfs:label>
		<skos:definition>severe or medically significant but not immediately life-threatening; hospitalization or prolongation of hospitalization indicated; disabling; limiting self care ADL</skos:definition>
		<idmp-dtp:hasDisplayName>Grade 3</idmp-dtp:hasDisplayName>
		<cmns-av:directSource>https://ctep.cancer.gov/protocoldevelopment/electronic_applications/docs/CTCAE_v5_Quick_Reference_5x7.pdf</cmns-av:directSource>
		<cmns-txt:hasTextValue>Grade 3</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-ctcae;CTCAE-Grade4">
		<rdf:type rdf:resource="&idmp-ctcae;CTCAE-GradeTerm"/>
		<rdfs:label>Grade 4</rdfs:label>
		<skos:definition>life-threatening consequences; urgent intervention indicated</skos:definition>
		<idmp-dtp:hasDisplayName>Grade 4</idmp-dtp:hasDisplayName>
		<cmns-av:directSource>https://ctep.cancer.gov/protocoldevelopment/electronic_applications/docs/CTCAE_v5_Quick_Reference_5x7.pdf</cmns-av:directSource>
		<cmns-txt:hasTextValue>Grade 4</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-ctcae;CTCAE-Grade5">
		<rdf:type rdf:resource="&idmp-ctcae;CTCAE-GradeTerm"/>
		<rdfs:label>Grade 5</rdfs:label>
		<skos:definition>death related to AE</skos:definition>
		<idmp-dtp:hasDisplayName>Grade 5</idmp-dtp:hasDisplayName>
		<cmns-av:directSource>https://ctep.cancer.gov/protocoldevelopment/electronic_applications/docs/CTCAE_v5_Quick_Reference_5x7.pdf</cmns-av:directSource>
		<cmns-txt:hasTextValue>Grade 5</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&idmp-ctcae;CTCAE-GradeTerm">
		<rdfs:subClassOf rdf:resource="&idmp-dtp;ISO21090-ConceptDescriptor"/>
		<rdfs:subClassOf rdf:resource="&cmns-cls;Classifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dsg;isDefinedIn"/>
				<owl:hasValue rdf:resource="&idmp-ctcae;CommonTerminologyCriteriaForAdverseEventsClassificationScheme"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Common Terminology Criteria for Adverse Events grade term</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:oneOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&idmp-ctcae;CTCAE-Grade1">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-ctcae;CTCAE-Grade2">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-ctcae;CTCAE-Grade3">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-ctcae;CTCAE-Grade4">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-ctcae;CTCAE-Grade5">
					</rdf:Description>
				</owl:oneOf>
			</owl:Class>
		</owl:equivalentClass>
		<skos:definition>classifier for the severity of an adverse event (AE)</skos:definition>
		<cmns-av:abbreviation>CTCAE grade term</cmns-av:abbreviation>
		<cmns-av:adoptedFrom>https://ctep.cancer.gov/protocoldevelopment/electronic_applications/docs/CTCAE_v5_Quick_Reference_5x7.pdf</cmns-av:adoptedFrom>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&idmp-ctcae;CommonTerminologyCriteriaForAdverseEventsClassificationScheme">
		<rdf:type rdf:resource="&cmns-cls;ClassificationScheme"/>
		<rdfs:label>Common Terminology Criteria for Adverse Events classification scheme</rdfs:label>
		<skos:definition>classification scheme describing criteria for the classification of adverse events by severity</skos:definition>
		<cmns-ra:isManagedBy rdf:resource="&idmp-nara;NationalCancerInstituteRegistrationAuthority"/>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-ctcae;CommonTerminologyCriteriaForAdverseEventsVocabulary">
		<rdf:type rdf:resource="&mvf;Vocabulary"/>
		<rdfs:label>Common Terminology Criteria for Adverse Events vocabulary</rdfs:label>
		<skos:definition>The NCI Common Terminology Criteria for Adverse Events is a descriptive terminology which can be utilized for Adverse Event (AE) reporting. A grading (severity) scale is provided for each AE term.</skos:definition>
		<skos:note>Entries in the vocabulary denoting adverse event types are registered in MedDRA.</skos:note>
		<cmns-av:abbreviation>CTCAE vocabulary</cmns-av:abbreviation>
		<cmns-av:synonym>CTCAE terminology</cmns-av:synonym>
		<cmns-av:synonym>Common Terminology Criteria for Adverse Events terminology</cmns-av:synonym>
		<cmns-ra:isManagedBy rdf:resource="&idmp-nara;NationalCancerInstituteRegistrationAuthority"/>
	</owl:NamedIndividual>

</rdf:RDF>