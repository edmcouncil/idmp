	<owl:Ontology rdf:about="https://spec.pistoiaalliance.org/idmp/ontology/ISO/NorthAmericanJurisdiction/PublicHealthInformationNetwork/">
		<rdfs:label>Public Health Information Network (PHIN) Ontology</rdfs:label>
		<dct:abstract>Public Health Information Network (PHIN) Ontology ontology extends the core IDMP ontologies to support the use of CDC mangaged controlled vocabularies.</dct:abstract>
		<dct:license rdf:datatype="&xsd;anyURI">https://opensource.org/licenses/MIT</dct:license>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11238-Substances/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/ISO11615-MedicinalProducts/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/NorthAmericanJurisdiction/NorthAmericanRegistrationAuthorities/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/NorthAmericanJurisdiction/NorthAmericanRegulatoryAgencies/"/>
		<owl:imports rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/META/ChangeManagement/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/AnnotationVocabulary/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Classifiers/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/CodesAndCodeSets/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Collections/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Designators/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Identifiers/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/ProductsAndServices/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/RegistrationAuthorities/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/TextDatatype/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
		<owl:versionIRI rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/20230814/NorthAmericanJurisdiction/PublicHealthInformationNetwork/"/>
		<idmp-chg:hasMaturityLevel rdf:resource="&idmp-chg;Provisional"/>
		<cmns-av:copyright>Copyright (c) 2022-2023 EDM Council, Inc.</cmns-av:copyright>
		<cmns-av:copyright>Copyright (c) 2022-2023 Pistoia Alliance, Inc.</cmns-av:copyright>
	</owl:Ontology>
	
	<owl:NamedIndividual rdf:about="&idmp-phin;PublicHealthInformationNetworkVocabularyAccessAndDistributionSystem">
		<rdf:type rdf:resource="&cmns-ra;RegistrationService"/>
		<rdf:type rdf:resource="&cmns-ra;Registry"/>
		<rdfs:label>Public Health Information Network Vocabulary Access and Distribution System</rdfs:label>
		<skos:definition>service and registry that provides standard vocabularies to the Center of Disease Control and Prevention (CDC) and its health partners</skos:definition>
		<cmns-av:acronym>PHIN-VADS</cmns-av:acronym>
		<cmns-prd:isProvidedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/NorthAmericanJurisdiction/NorthAmericanRegistrationAuthorities/CentersForDiseaseControlAndPreventionRegistrationAuthority"/>
		<cmns-ra:isManagedBy rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/NorthAmericanJurisdiction/NorthAmericanRegistrationAuthorities/CentersForDiseaseControlAndPreventionRegistrationAuthority"/>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&idmp-phin;PublicHealthInformationNetworkVocabularyAccessAndDistributionSystemClassificationScheme">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-ra;isRegisteredIn"/>
				<owl:hasValue rdf:resource="&idmp-phin;PublicHealthInformationNetworkVocabularyAccessAndDistributionSystem"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdf:label>Public Health Information Network Vocabulary Access and Distribution System classification scheme</rdf:label>
		<rdf:subClassOf rdf:resource="&cmns-cls;ClassificationScheme"/>
		<skos:definition>classification scheme registered in the Public Health Information Network Vocabulary Access and Distribution System</skos:definition>
		<cmns-av:abbreviation>PHIN-VADS classification scheme</cmns-av:abbreviation>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&idmp-phin;VADS-RaceAndEthnicityCDC">
		<rdfs:label>PHIN-VADS Race and Ethnicity - CDC</rdfs:label>
		<rdfs:type rdf:resource="&idmp-phin;PublicHealthInformationNetworkVocabularyAccessAndDistributionSystemClassificationScheme"/>
		<skos:definition>code system for race and ethnicity managed in PHIN-VADS</skos:definition>
		<cmns-av:directSource rdf:resource="https://phinvads.cdc.gov/vads/ViewCodeSystem.action?id=2.16.840.1.113883.6.238"/>
		<cmns-id:isIdentifiedBy>
			<owl:NamedIndividual>
				<rdf:type rdf:resource="&idmp-phin;VADS-CodeSystemCode"/>
				<cmns-txt:hasTextValue>PH_RaceAndEthnicity_CDC</cmns-txt:hasTextValue>
			</owl:NamedIndividual>
		</cmns-id:isIdentifiedBy>
		<cmns-id:isIdentifiedBy>
			<owl:NamedIndividual>
				<rdf:type rdf:resource="&idmp-phin;VADS-CodeSystemOID"/>
				<cmns-txt:hasTextValue>2.16.840.1.113883.6.238</cmns-txt:hasTextValue>
			</owl:NamedIndividual>
		</cmns-id:isIdentifiedBy>
		<mvf:hasURI>https://phinvads.cdc.gov/vads/ViewCodeSystem.action?id=2.16.840.1.113883.6.238</mvf:hasURI>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&idmp-phin;VADS-RaceAndEthnicityCDC-Type">
		<rdfs:subClassOf rdf:resource="&idmp-phin;VADS-CodeSystemConcept"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/NorthAmericanJurisdiction/PublicHealthInformationNetwork/cmns-col;isIncludedIn"/>
				<owl:hasValue rdf:resource="&idmp-phin;VADS-RaceAndEthnicityCDC"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Race &amp; Ethnicity - CDS type</rdfs:label>
		<skos:definition>classifier and code element that describes the race and ethnicity of humans</skos:definition>
		<cmns-av:usageNote>can be used for the characterization of target populations</cmns-av:usageNote>
	</owl:Class>
	
	<rdf:Description rdf:about="&idmp-phin;VADSCodeSystem">
		<rdf:type rdf:resource="&owl;Class"/>
		<rdf:type rdf:resource="&cmns-cls;ClassificationScheme"/>
		<rdf:type rdf:resource="&cmns-cds;CodeSet"/>
		<rdfs:subClassOf rdf:resource="&idmp-dtp;ControlledVocabulary"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-id;isIdentifiedBy"/>
				<owl:onClass rdf:resource="&idmp-phin;VADSCodeSystemCode"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-id;isIdentifiedBy"/>
				<owl:onClass rdf:resource="&idmp-phin;VADSCodeSystemOID"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Public Health Information Network Vocababulary and Distribution System (PHIN-VADS) coding scheme</rdfs:label>
		<skos:definition>classification scheme in the Public Health Information Network Vocababulary and Distribution System</skos:definition>
	</rdf:Description>
	
	<owl:Class rdf:about="&idmp-phin;VADSCodeSystemConcept">
		<rdfs:subClassOf rdf:resource="&idmp-dtp;ISO21090-ConceptDescriptor"/>
		<rdfs:subClassOf rdf:resource="&cmns-cls;Classifier"/>
		<rdfs:subClassOf rdf:resource="&mvf;MVFEntry"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dsg;isSignifiedBy"/>
				<owl:onClass rdf:resource="&idmp-phin;VADSPreferredConceptName"/>
				<owl:maxQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&mvf;hasBroaderEntry"/>
				<owl:onClass rdf:resource="&idmp-phin;VADSCodeSystemConcept"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&mvf;hasNarrowerEntry"/>
				<owl:onClass rdf:resource="&idmp-phin;VADSCodeSystemConcept"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isIncludedIn"/>
				<owl:onClass rdf:resource="&idmp-phin;VADSCodeSystem"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dsg;isSignifiedBy"/>
				<owl:onClass rdf:resource="&idmp-phin;VADSConceptName"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-id;isIdentifiedBy"/>
				<owl:onClass rdf:resource="&idmp-phin;VADSCodeSystemConceptCode"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Public Health Information Network Vocababulary and Distribution System (PHIN-VADS) code system concept</rdfs:label>
		<skos:definition>concept in a controlled vocabulary (code system) managed by the Public Health Information Network initiative</skos:definition>
		<cmns-av:abbreviation>PHIN-VADS coding scheme concept</cmns-av:abbreviation>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-phin;VADSConceptName">
		<rdfs:subClassOf rdf:resource="&cmns-dsg;Name"/>
		<rdfs:subClassOf rdf:resource="&mvf;VocabularyEntry"/>
		<rdfs:label>PHIN-VADS concept name</rdfs:label>
		<rdf:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isIncludedIn"/>
				<owl:allValuesFrom rdf:resource="&idmp-phin;VADSCodeSystem"/>
			</owl:Restriction>
		</rdf:subClassOf>
		<rdf:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dsg;denotes"/>
				<owl:allValuesFrom rdf:resource="&idmp-phin;VADSCodeSystemConcept"/>
			</owl:Restriction>
		</rdf:subClassOf>
		<skos:definition>name designating a concept in a PHIN-VADS code system</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-phin;VADSConceptSystemCode">
		<rdfs:subClassOf rdf:resource="&cmns-id;RegisteredIdentifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-txt;hasTextValue"/>
				<owl:onDatatype rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/NorthAmericanJurisdiction/PublicHealthInformationNetwork/xsd;string"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>PHIN-VADS code system code</rdfs:label>
		<skos:definition>registered identifier in the PHIN-VADS registry for a concept system in PHIN-VADS</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-phin;VADSConceptSystemConceptCode">
		<rdfs:subClassOf rdf:resource="&mvf;TermStatus"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-txt;hasTextValue"/>
				<owl:onDatatype rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/NorthAmericanJurisdiction/PublicHealthInformationNetwork/xsd;string"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>PHIN-VADS code system concept code</rdfs:label>
		<skos:definition>registered identifier in the PHIN-VADS registry for a concept in a PHIN-VADS concept scheme</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-phin;VADSConceptSystemOID">
		<rdfs:subClassOf rdf:resource="&idmp-dtp;ObjectIdentifier"/>
		<rdfs:subClassOf rdf:resource="&cmns-id;RegisteredIdentifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-txt;hasTextValue"/>
				<owl:onDatatype rdf:resource="https://spec.pistoiaalliance.org/idmp/ontology/ISO/NorthAmericanJurisdiction/PublicHealthInformationNetwork/xsd;string"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>PHIN-VADS code system OID</rdfs:label>
		<skos:definition>registered identifier in an ISO OID registry for a concept system in PHIN-VADS</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-phin;VADSPreferredConceptName">
		<rdfs:subClassOf rdf:resource="&idmp-phin;VADSConceptName"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&mvf;isPreferred"/>
				<owl:hasValue rdf:datatype="&xsd;boolean">true</owl:hasValue>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>PHIN-VADS preferred concept name</rdfs:label>
		<skos:definition>preferred name designating a concept in a PHIN-VADS code system</skos:definition>
	</owl:Class>

</rdf:RDF>