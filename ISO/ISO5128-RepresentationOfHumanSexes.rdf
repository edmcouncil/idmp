		<mvf:isPreferred rdf:datatype="&xsd;boolean">true</mvf:isPreferred>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&idmp-sex;ISO5128HumanSex">
		<rdfs:subClassOf rdf:resource="&idmp-dtp;ISO21090-ConceptDescriptor"/>
		<rdfs:subClassOf rdf:resource="&mvf;MVFEntry"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isIncludedIn"/>
				<owl:hasValue rdf:resource="&idmp-sex;ISO5128Vocabulary"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>ISO 5128 human sex</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:oneOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&idmp-sex;ISO5128-Female">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-sex;ISO5128-Male">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-sex;ISO5128-NotApplicable">
					</rdf:Description>
					<rdf:Description rdf:about="&idmp-sex;ISO5128-NotKnown">
					</rdf:Description>
				</owl:oneOf>
			</owl:Class>
		</owl:equivalentClass>
		<skos:definition>concept for the classification of human sex or the availability of information about it</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&idmp-sex;ISO5128HumanSexName">
		<rdfs:subClassOf rdf:resource="&mvf;VocabularyEntry"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isIncludedIn"/>
				<owl:hasValue rdf:resource="&idmp-sex;ISO5128Vocabulary"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dsg;denotes"/>
				<owl:onClass rdf:resource="&idmp-sex;ISO5128HumanSex"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>ISO 51128 human sex name</rdfs:label>
		<skos:definition>concept name for the classification of human sex or the availability of information about it</skos:definition>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5128Vocabulary">
		<rdf:type rdf:resource="&idmp-dtp;ControlledVocabulary"/>
		<rdf:type rdf:resource="&cmns-cds;CodeSet"/>
		<rdf:type rdf:resource="&mvf;Vocabulary"/>
		<rdfs:label>ISO 5128 codes for the representation of human sex</rdfs:label>
	</owl:NamedIndividual>
	
	<owl:DatatypeProperty rdf:about="&idmp-sex;hasISO5128Code">
		<rdfs:label>has ISO 5128 code</rdfs:label>
		<rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
		<skos:definition>single digit integer value denoting a human sex according to ISO 5128</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&mvf;hasTextualName">
	</owl:DatatypeProperty>

