		<idmp-sex:hasISO5128Code rdf:datatype="&xsd;nonNegativeInteger">1</idmp-sex:hasISO5128Code>
		<cmns-dsg:isSignifiedBy rdf:resource="&idmp-sex;ISO5128-Male-Name"/>
		<cmns-txt:hasTextValue>Male</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
