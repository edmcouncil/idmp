		<mvf:isPreferred rdf:datatype="&xsd;boolean">true</mvf:isPreferred>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5128-NotApplicable">
		<rdf:type rdf:resource="&idmp-sex;ISO5128HumanSex"/>
		<rdfs:label>ISO 5128 - Not applicable</rdfs:label>
		<skos:definition>concept descriptor to be used to indicate that the human sex is not applicable in the context that the concept is used</skos:definition>
		<skos:note>this concept is not a classifier</skos:note>
		<idmp-dtp:hasDisplayName>Not applicable</idmp-dtp:hasDisplayName>
		<idmp-sex:hasISO5128Code rdf:datatype="&xsd;nonNegativeInteger">9</idmp-sex:hasISO5128Code>
		<cmns-dsg:isSignifiedBy rdf:resource="&idmp-sex;ISO5128-NotApplicable-Name"/>
		<cmns-txt:hasTextValue>Not applicable</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5128-NotApplicable-Name">
		<rdf:type rdf:resource="&idmp-sex;ISO5128HumanSexName"/>
		<rdfs:label>ISO 5128 - Not applicable (en)</rdfs:label>
		<mvf:hasTextualName xml:lang="en">Not applicable</mvf:hasTextualName>
		<mvf:isPreferred rdf:datatype="&xsd;boolean">true</mvf:isPreferred>
