	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5128-Female">
		<rdf:type rdf:resource="&idmp-sex;ISO5128HumanSex"/>
		<rdf:type rdf:resource="&cmns-cls;Classifier"/>
		<rdfs:label>ISO 5128 - Female</rdfs:label>
		<skos:definition>concept descriptor to be used to indicate that the human sex is female</skos:definition>
		<skos:note>this concept is a classifier for humans based on their sex being female</skos:note>
		<idmp-dtp:hasDisplayName>Female</idmp-dtp:hasDisplayName>
		<idmp-sex:hasISO5128Code rdf:datatype="&xsd;nonNegativeInteger">2</idmp-sex:hasISO5128Code>
		<cmns-dsg:isSignifiedBy rdf:resource="&idmp-sex;ISO5128-Female-Name"/>
		<cmns-txt:hasTextValue>Female</cmns-txt:hasTextValue>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5128-Female-Name">
		<rdf:type rdf:resource="&idmp-sex;ISO5128HumanSexName"/>
		<rdfs:label>ISO 5128 - Female (en)</rdfs:label>
		<mvf:hasTextualName xml:lang="en">Female</mvf:hasTextualName>
		<mvf:isPreferred rdf:datatype="&xsd;boolean">true</mvf:isPreferred>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&idmp-sex;ISO5128-Male">
