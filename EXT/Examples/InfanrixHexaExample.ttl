idmp-infhx:  rdf:type              owl:Ontology ;
        rdfs:label                 "Infanrix Hexa Example Ontology" ;
        dct:abstract               "The ontology provides an instance-based example for some of the data related to the multi-component vaccine, Infanrix Hexa, which has been available in the EU for many years, to work through the basis of strength representation of a drug with multiple active ingredients." ;
        dct:license                "https://opensource.org/licenses/MIT"^^xsd:anyURI ;
        owl:imports                idmp-ucum: , idmp-eura: , idmp-eurga: , idmp-ra: , idmp-sub: , idmp-uom: , idmp-mprd: , idmp-dtp: , idmp-chg: , cmns-av: , cmns-cls: , cmns-col: , cmns-cxtdsg: , cmns-dsg: , cmns-doc: , cmns-ge-euj: , cmns-id: , cmns-org: , cmns-pts: , cmns-prd: , cmns-qtu: , cmns-ra: , cmns-rga: , cmns-txt: , lcc-cr: , lcc-3166-1: , lcc-639-1: , lcc-lr: ;
        owl:versionIRI             <https://spec.pistoiaalliance.org/idmp/ontology/EXT/20230601/Examples/InfanrixHexaExample/> ;
        idmp-chg:hasMaturityLevel  idmp-chg:Informative ;
        cmns-av:copyright          "Copyright (c) 2022-2023 EDM Council, Inc." , "Copyright (c) 2022-2023 Pistoia Alliance, Inc." ;
		cmns-av:usageNote "This example is for illustration purposes only. The data provided may not be up-to-date, and in some cases, 'dummy' data has been included. The product information included here shall not be used for any other purpose than for educational purposes regarding the IDMP-O implementation." ;
