	<owl:NamedIndividual rdf:about="&idmp-cc;CellCept-TherapeuticIndication-HeartTransplantRejection">
		<rdf:type rdf:resource="&idmp-mprd;TherapeuticIndication"/>
		<idmp-mprd:hasIndicationAsDiseaseSymptomProcedure>
			<owl:NamedIndividual>
				<rdf:type rdf:resource="&idmp-eura;MedDRATerm"/>
				<rdf:type rdf:resource="&idmp-mprd;IndicationAsDiseaseSymptomProcedure"/>
				<rdfs:label>Prophylaxis against heart transplant rejection</rdfs:label>
				<cmns-cls:isClassifiedBy rdf:resource="&idmp-eura;MedDRATermClassifier-LowestLevelTerm"/>
				<cmns-id:isIdentifiedBy>
					<idmp-eura:MedDRAIdentifier>
						<cmns-txt:hasTextValue>10050432</cmns-txt:hasTextValue>
					</idmp-eura:MedDRAIdentifier>
				</cmns-id:isIdentifiedBy>
			</owl:NamedIndividual>
		</idmp-mprd:hasIndicationAsDiseaseSymptomProcedure>
		<idmp-mprd:hasTherapeuticIndicationText rdf:resource="&idmp-cc;CellCeptTherapeuticIndicationText"/>
		<idmp-mprd:isTherapeuticIndicationFor rdf:resource="&idmp-cc;CellCeptMedicinalProduct"/>
	</owl:NamedIndividual>
	
