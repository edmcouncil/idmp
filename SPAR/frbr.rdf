	<owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#responsibleEntityOf">
		<rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
		<rdfs:label xml:lang="en">is responsible entity of</rdfs:label>
