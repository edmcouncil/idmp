						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&doco;Section">
							</rdf:Description>
							<rdf:Description rdf:about="&po;HeadedContainer">
							</rdf:Description>
						</owl:intersectionOf>
					</owl:Class>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&dct;isPartOf"/>
						<owl:someValuesFrom rdf:resource="&doco;BackMatter"/>
					</owl:Restriction>
				</owl:intersectionOf>
			</owl:Class>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&dct;hasPart"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&doco;ListOfReferences">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&po;contains"/>
