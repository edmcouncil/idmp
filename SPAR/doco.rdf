				</owl:intersectionOf>
			</owl:Class>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en">formula box</rdfs:label>
		<rdfs:comment xml:lang="en">A space within a document that contains one or more formulae.</rdfs:comment>
	</owl:Class>
	
	<owl:Class rdf:about="&doco;FrontMatter">
		<rdfs:subClassOf>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&deo;DiscourseElement">
					</rdf:Description>
					<rdf:Description rdf:about="&po;Container">
					</rdf:Description>
				</owl:intersectionOf>
			</owl:Class>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&po;isContainedBy"/>
				<owl:allValuesFrom>
					<owl:Class>
						<owl:complementOf>
							<owl:Class>
								<owl:unionOf rdf:parseType="Collection">
									<rdf:Description rdf:about="&doco;BackMatter">
									</rdf:Description>
									<rdf:Description rdf:about="&doco;BodyMatter">
									</rdf:Description>
									<rdf:Description rdf:about="&doco;FrontMatter">
